}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].EngagementName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].EngagementIdentifier)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].PHI)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].PCI)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].UpStreamSystem)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].DownStreamSystem)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].ProjectStartDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].ARBDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].ReceiveDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].StartDate)
