                    @Html.DisplayNameFor(model => model.Initiative[0].LastModifiedDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Initiative[0].IsActive)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Initiative[0].EngagementTypeNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Initiative[0].LocationNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Initiative[0].ResourceNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Initiative[0].SolutionTypeNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Initiative) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EngagementName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EngagementIdentifier)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PHI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PCI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpStreamSystem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DownStreamSystem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProjectStartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ARBDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReceiveDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompleteDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedUserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastModifiedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EngagementTypeNavigation.EngagementTypeDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LocationNavigation.LocationDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResourceNavigation.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SolutionTypeNavigation.ModifiedUserName)
            </td>
            <td>
                <a asp-page="/Notes/Index" asp-route-id="@item.InitiativeId">Notes</a>
                <a asp-page="./Edit" asp-route-id="@item.InitiativeId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.InitiativeId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.InitiativeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
